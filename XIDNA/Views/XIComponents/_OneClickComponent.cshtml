    function GetDataTable(Fields, Optrs, Values, Type, NVPairs, oneClickID, TableColumns, tabGUID, sGUID, ActiveTabParam) {
        if('@o1ClickD.ResultListDisplayType'==1){
            var SortColumns =TableColumns.substring(2,TableColumns.length-2).split('","');
            var expColumns = new Array();
            for (var i = 0; i < SortColumns.length; i++) {
                var obj = new Object();
                obj.sName = SortColumns[i];
                if (SortColumns[i] == "Select") {
                    obj.bSortable = false;
                }
                else {
                    obj.bSortable = true;
                }
                if (obj.sName == 'HiddenData') {
                    obj.visible = false;
                }
                expColumns.push(obj);
            }
            var iPageLength = 0;
            if('@o1ClickD.iPaginationCount' == 0)
            {
                iPageLength = 100;
            }
            else
            {
                iPageLength = '@o1ClickD.iPaginationCount';
            }
            var DynamicParams ="";
            if(NVPairs && NVPairs.length > 0){
                DynamicParams = NVPairs.join(",");
            }
            fncGetDTable(OneClickURL, oneClickID, expColumns, sGUID, '@o1ClickD.SearchText', iPageLength, tabGUID, '@o1ClickD.SearchType', '@o1ClickD.sParentWhere', DynamicParams, '@o1ClickD.iDefaultSortColumn', '@o1ClickD.sDefaultSortDirection', '@o1ClickD.bIsSelectAll', ActiveTabParam, '@o1ClickD.sNoResultsMessage');
        }
        else{
            $("#OneClickResult-@sTabGUID").tablesorter();
        }
    }

