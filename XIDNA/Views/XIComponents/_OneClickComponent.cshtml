            }
            if (iRunType == 30) {
                XIRun(_this, XiLinkID, ID, sGUID, BO, IsMerge, BODID, '', '', InputParams);
            }
            else if (iRunType == 40) {
                XILinkLoadJson(XiLinkID, sGUID, InputParams);
            }
        }
    }
    function fncclose($this, iId) {
        $('#' + iId + '').remove()
    }
</script>
